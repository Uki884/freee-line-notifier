{
  "name": "@freee-line-notifier/monorepo",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "pnpm": "9.13.2",
    "npm": "Please use pnpm!",
    "yarn": "Please use pnpm!"
  },
  "scripts": {
    "dev": "pnpm run --parallel --recursive --if-present dev",
    "deploy": "pnpm run --parallel --recursive --if-present deploy",
    "lint": "biome lint -- apply ./",
    "format": "biome format ./ --write",
    "fix": "biome check --write ./",
    "typecheck": "pnpm run --parallel --recursive --if-present typecheck"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "wrangler": "3.106.0"
  },
  "packageManager": "pnpm@9.13.2"
}
